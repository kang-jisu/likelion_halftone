
<h1>글쓰기</h1>
<hr>
<form action="/post/create" method="post" class="form-group">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    작성자 <input type="text" name="writer"class="form-control" ><br>
    Title<br>
    <input type="text" name="title" class="form-control" placeholder="title" size="50"><br>
   
    Content<br>
    <textarea name="content" class="form-control" placeholder="content" rows="10" cols="50"></textarea> <br>
    
    <input type="file" name="file" class="form-control"><br><br>
    
    <input type="submit" value="확인" class="btn btn-primary">
    <input type="button" value="취소" class="btn btn-primary" onclick='location.href="/"'>
</form>

